@model IEnumerable<MVCDemo2.Models.Employee>
@{
    ViewBag.Title = "Employees List";
}

<h2>Employees List</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
            </tr>
        }

    </tbody>
</table>
